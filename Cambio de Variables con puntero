#include<iostream>
using namespace std;
int main()
{
    int a=3, b=5, *pa=&a, *pb=&b, temp;
    cout<<"El valor de a es: "<<a<<endl;
    cout<<"El valor de b es: "<<b<<endl;
    temp=*pb;
    *pb=*pa;
    *pa=temp;
    
    cout<<"El cambio es: \n";
    cout<<"Valor de a: "<<a<<endl;
    cout<<"Valor de b: "<<b<<endl;
return 0;
}
