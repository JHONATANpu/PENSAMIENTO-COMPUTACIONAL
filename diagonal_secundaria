#include<iostream>
using namespace std;
int main()
{
    int n, i, j,S=0;
    cout<<"Valor de n: "; cin >>n;
    int A[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            cin>>A[i][j];
        }cout<<endl;
    }
    for(i=0;i<n;i++){
        S+=A[i][n-1-i];
    }
    cout<<S;
    return 0;
}
